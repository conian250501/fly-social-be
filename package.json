{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "lint": "eslint src/**/*.ts",
    "start": "yarn lint && yarn build && node dist/index.js",
    "dev": "nodemon src/index.ts --watch",
    "db:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/database/data-source.ts --pretty",
    "db:migrate": "typeorm-ts-node-commonjs migration:run -d ./src/database/data-source.ts",
    "db:rollback": "typeorm-ts-node-commonjs migration:revert -d ./src/database/data-source.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.1341.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^6.6.6",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "2",
    "passport": "^0.6.0",
    "passport-facebook-token": "^4.0.0",
    "passport-github-token": "^3.0.0",
    "passport-google-oauth-token": "^1.0.6",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.3",
    "@types/node-fetch": "^2.6.3",
    "@types/passport": "^1.0.12",
    "@types/passport-facebook-token": "^0.4.35",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "typescript": "~4.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "author": "conian-guys",
  "license": "ISC"
}
